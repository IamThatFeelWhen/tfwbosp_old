#######Energy Torpedoes 3######
# PROJECTILE

entity = {
	name = "tfw_deiterium_torpedoes_entity"
	cull_radius = 1.0
	
	default_state = idle
	state = { name = "idle"
		event = { time = 0 particle = "tfw_deiterium_torpedoes_idle_particle" keep_particle = yes }
		event = { time = 0 particle = "tfw_deiterium_torpedoes_idle_trail_particle" keep_particle = yes }
	}
}

# HIT EFFECT

entity = {
	name = "tfw_deiterium_torpedoes_hit_entity"
	cull_radius = 1.0
	
	state = { name = "explode_med_hp" state_time = 5 looping = no
		event = { time = 0.0 node = "root" particle = "tfw_deiterium_torpedoes_hit_effect" sound = { soundeffect = energy_torpedo_hit } }
		event = { trigger_once = yes sound = { soundeffect = "distance_energy_torpedo_hit" } } 
	}
	default_state = "explode_med_hp"
}

